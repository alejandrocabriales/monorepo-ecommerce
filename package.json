{
  "name": "monorepo-ecomerce",
  "packageManager": "yarn@3.6.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "front": "cd packages/ecommerce-front/src && yarn dev",
    "backend": "cd packages/ecommerce-backend/src && yarn start",
    "start": "concurrently \"yarn front\" \"yarn backend\""
  }
}
